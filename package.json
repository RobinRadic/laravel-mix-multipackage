    "version": "1.0.0",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/robinradic/laravel-mix-multipackage"
    },
    "homepage": "https://github.com/robinradic/laravel-mix-multipackage",
    "maintainers": [
        {
            "name": "Robin Radic",
            "email": "rradic@hotmail.com",
            "url": "https://github.com/RobinRadic"
        }
    ],
    "license": "MIT",
